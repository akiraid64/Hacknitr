{
  "version": 2,
  "name": "ecolink-supply-chain",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SOLANA_NETWORK": "devnet",
    "NEXT_PUBLIC_SOLANA_RPC_URL": "https://api.devnet.solana.com"
  }
}
